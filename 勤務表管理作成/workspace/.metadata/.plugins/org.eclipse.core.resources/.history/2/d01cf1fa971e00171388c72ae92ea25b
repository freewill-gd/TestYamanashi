	<logic:iterate id="row" name="testActionForm" property="data">
		<logic:iterate id="col" name="row" property="row">
			<bean:write name="col"  property="key" />
			<bean:write name="col" property="key" />
			<logic:equal name="col" property="key" value="work_date">
				<bean:write name="col" property="value" />
			</logic:equal>
		</logic:iterate>
		<br>
	</logic:iterate>
	end<br>
