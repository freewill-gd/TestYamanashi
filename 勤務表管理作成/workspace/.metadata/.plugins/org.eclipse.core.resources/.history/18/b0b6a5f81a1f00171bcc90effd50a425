  <select id="selectT_roster" resultType="t_rosterResultMap">
<![CDATA[
SELECT
  'aaa' as  hoge
FROM
  t_roster
WHERE
 work_date >= TO_DATE('2017/04', 'YYYY/MM')
 AND work_date < TO_DATE('2017/05', 'YYYY/MM')
]]>

  </select>
